{
    "name": "moderate-fw",
    "description": "Slim-framework extension: Moderate Framework",
    "require": {
        "php": "~5.6.7",
        "slim/slim": "~2.6.2",
        "slim/extras": "2.0.*",
        "j4mie/idiorm": "1.*",
        "twig/twig": "1.*",
        "twig/extensions": "1.*",
        "smarty/smarty": "~3.1.27",
        "illuminate/session": "~5.1.22",
        "illuminate/filesystem": "~5.1.22",
        "illuminate/validation": "~5.1.22",
        "Illuminate/translation": "~5.1.22",
        "j4mie/paris": "~1.5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "5.0.*",
        "phpunit/phpunit-skeleton-generator": "*"
    },
   "autoload": {
        "psr-4": {
            "MfBase\\": "lib/Moderate/Wrapper/",
            "MfPackage\\": "app/",
            "MfMiddleware\\": "lib/Moderate/Middleware",
            "MfModels\\": "app/Models/",
            "MfController\\": "app/Controllers/",
            "MfValidations\\": "app/Validations/",
            "MfTestsResources\\": "tests/"
        }
   }
}
